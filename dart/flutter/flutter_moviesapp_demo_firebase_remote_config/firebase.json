{
  "hosting": [
    {
      "target": "moviesapp-demo-firebase-remote-config",
      "public": "build/web",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}